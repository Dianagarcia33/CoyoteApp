{
  "expo": {
    "name": "CoyoteApp",
    "slug": "CoyoteApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "coyoteapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dianadeveloper.coyoteapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/coyote-logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0b111e"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/coyote-logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0b111e"
        }
      ],
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#FF6A3D",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2ddc227d-ecf5-4227-b041-2d4a92c2858b"
      }
    },
    "owner": "dianadeveloper",
    "newArchEnabled": false
  }
}
